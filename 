[32m+[m[32mclass TreeNode{[m
[32m+[m[32m    int val;[m
[32m+[m[32m    TreeNode left;[m
[32m+[m[32m    TreeNode right;[m
[32m+[m
[32m+[m[32m    public TreeNode(int val){[m
[32m+[m[32m        this.val = val;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
