[32m+[m[32m                    blockingQueue.wait();[m
[32m+[m[32m                }[m
[32m+[m[32m                int res = blockingQueue.pollFirst();[m
[32m+[m[32m                blockingQueue.notifyAll();[m
[32m+[m[32m                return res;[m
[32m+[m
